# NextAuth
# Generate with: node generate-secrets.js
NEXTAUTH_SECRET="your_128_character_hex_secret_for_nextauth_jwt_signing"
NEXTAUTH_URL="https://orbitspace.org"

# FastAPI Backend
FASTAPI_URL="https://api.orbitspace.org"
# Generate with: node generate-secrets.js
FASTAPI_SECRET_KEY="your_128_character_hex_secret_for_fastapi_jwt_and_sessions"

# Anthropic Claude API
ANTHROPIC_API_KEY="your_anthropic_api_key"

# Redis (Upstash)
REDIS_URL="https://your-upstash-redis-url.upstash.io"
REDIS_TOKEN="your_upstash_redis_token"

# Internal API Secret
# Generate with: node generate-secrets.js
NEXTJS_API_SECRET="your_base64_encoded_internal_api_secret"
FASTAPI_CORS_ORIGINS="https://orbitspace.org,https://www.orbitspace.org,https://api.orbitspace.org"

# Encryption
# Generate with: node generate-secrets.js
ENCRYPTION_KEY="your_64_character_hex_encryption_key_for_api_key_storage"

# Workspace
WORKSPACE_ROOT="/workspaces"
WORKSPACE_MAX_SIZE_GB=10

# Git Bot
GIT_BOT_NAME="OrbitSpace-bot"
GIT_BOT_EMAIL="bot@orbitspace.org"

# Security
ALLOWED_BASH_COMMANDS="ls,cat,grep,git,npm,python,node,pytest,jest"
BASH_TIMEOUT_SECONDS=60
MAX_FILE_READ_SIZE_MB=1

# Agent Configuration
RESEARCH_AGENT_MODEL="claude-sonnet-4"
PLANNING_AGENT_MODEL="claude-sonnet-4"
IMPLEMENTATION_AGENT_MODEL="claude-haiku-4"
REVIEW_AGENT_MODEL="claude-sonnet-4"
MAX_CONCURRENT_AGENTS=5
