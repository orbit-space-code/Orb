# Database
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/orbitspace_compyle"

# GitHub OAuth App
GITHUB_CLIENT_ID="your_github_app_client_id"
GITHUB_CLIENT_SECRET="your_github_app_client_secret"
GITHUB_APP_ID="your_github_app_id"
GITHUB_APP_PRIVATE_KEY="-----BEGIN RSA PRIVATE KEY-----\n...\n-----END RSA PRIVATE KEY-----"

# NextAuth
NEXTAUTH_SECRET="random_secret_key_at_least_32_characters"
NEXTAUTH_URL="http://localhost:3000"

# FastAPI Backend
FASTAPI_URL="http://localhost:8000"

# Anthropic Claude API
ANTHROPIC_API_KEY="your_anthropic_api_key"

# Redis
REDIS_URL="redis://localhost:6379"

# Internal API Secret
NEXTJS_API_SECRET="shared_secret_for_internal_api_calls"

# Workspace
WORKSPACE_ROOT="/workspaces"
WORKSPACE_MAX_SIZE_GB=10

# Git Bot
GIT_BOT_NAME="Compyle Bot"
GIT_BOT_EMAIL="bot@compyle.dev"

# Security
ALLOWED_BASH_COMMANDS="ls,cat,grep,git,npm,python,node,pytest,jest"
BASH_TIMEOUT_SECONDS=60
MAX_FILE_READ_SIZE_MB=1

# Agent Configuration
RESEARCH_AGENT_MODEL="claude-sonnet-4"
PLANNING_AGENT_MODEL="claude-sonnet-4"
IMPLEMENTATION_AGENT_MODEL="claude-haiku-4"
REVIEW_AGENT_MODEL="claude-sonnet-4"
MAX_CONCURRENT_AGENTS=5
